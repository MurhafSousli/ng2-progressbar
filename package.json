    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "^2.4.2",
    "protractor": "5.3.1",
    "standard-version": "^4.3.0",
    "ts-node": "6.0.2",
    "tsickle": "^0.27.5",
    "tslint": "5.10.0",
    "typescript": "~2.7.2",
